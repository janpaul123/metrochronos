{
  "name": "metrochronos",
  "version": "0.0.1",
  "description": "Run Javascript on pieces of paper!",
  "authors": ["Jan Paul Posma <j@npaulpos.ma>", "Noah Tye <hi@noahtye.com>"],
  "license": "MIT",
  "scripts": {
    "dev": "npm run createdb && npm run migrate && npm start",
    "dev:docker": "npm run migrate && npm start",
    "createdb": "psql -c \"SELECT 1\" metrochronos_development 2>/dev/null 1>/dev/null || createdb metrochronos_development",
    "migrate": "knex migrate:latest",
    "start": "node server/entry-server.js",
    "dropdb": "dropdb --if-exists metrochronos_development",
    "heroku-postbuild": "webpack",
    "test": "npm run lint",
    "lint": "eslint ."
  },
  "engines": {
    "node": "9.3.x",
    "npm": "5.6.x"
  },
  "cacheDirectories": [
    ".babel-cache"
  ],
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "blob-stream": "^0.1.3",
    "brfs": "^1.5.0",
    "color-diff": "^1.1.0",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "d3": "^5.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "heroku-ssl-redirect": "0.0.4",
    "json-loader": "^0.5.7",
    "knex": "^0.14.4",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "nocache": "^2.0.0",
    "node-matrices": "^1.0.0",
    "pdfkit": "^0.8.3",
    "pg": "^7.4.1",
    "randomcolor": "^0.5.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-monaco-editor": "^0.13.0",
    "shuffle-seed": "^1.1.6",
    "style-loader": "^0.19.1",
    "transform-loader": "^0.2.4",
    "uuid": "^3.2.1",
    "webpack": "^3.11.0",
    "xhr": "^2.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.1",
    "prettier": "^1.11.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-dev-server": "^2.11.2"
  }
}
